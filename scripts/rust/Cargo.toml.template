[package]
name = "proto-demo"
version = "1.0.0"
edition = "2021"
description = "Generated protobuf/gRPC library for proto-demo"
license = "MIT"

[lib]
name = "proto_demo"

[dependencies]
prost = "0.12"
tonic = { version = "0.11", features = ["transport"] }
serde = { version = "1", features = ["derive"] }
serde_json = "1"

[build-dependencies]
tonic-build = "0.11"
